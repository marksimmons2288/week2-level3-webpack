{
  "name": "week2-level3-webpack",
  "version": "1.0.0",
  "description": "This is a project to practice webpack configuration and usage.",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "check:node": "node scripts/checkEnv.js hello-from-npm-script",
    "dev": "webpack serve --open --mode development",
    "build": "webpack --mode production",
    "lint:js": "eslint \"src/**/*.js\"",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:html": "htmlhint \"src/index.html\"",
    "lint": "npm run lint:js && npm run lint:scss && npm run lint:html",
    "lint:fix:js": "eslint \"src/**/*.js\" --fix",
    "lint:fix:scss": "stylelint \"src/**/*.scss\" --fix",
    "lint:fix": "npm run lint:fix:js && npm run lint:,fix:scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marksimmons2288/week2-level3-webpack.git"
  },
  "author": "mark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marksimmons2288/week2-level3-webpack/issues"
  },
  "homepage": "https://github.com/marksimmons2288/week2-level3-webpack#readme",
  "devDependencies": {
    "css-loader": "^7.1.2",
    "eslint": "^9.39.1",
    "html-webpack-plugin": "^5.6.5",
    "htmlhint": "^1.8.0",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "sass": "^1.94.2",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "stylelint": "^16.26.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
